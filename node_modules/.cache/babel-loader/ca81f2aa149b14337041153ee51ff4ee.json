{"ast":null,"code":"// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue';\nimport axios from 'axios';\nexport default {\n  el: '#app',\n\n  data() {\n    return {\n      items: []\n    };\n  },\n\n  components: {\n    HelloWorld\n  },\n  methods: {\n    dohvatiItem() {\n      axios.get('http://127.0.0.1:5000/app/cart').then(response => {\n        console.log('Ovo su podaci koji se dohvacaju' + response.data.data);\n        this.items = response.data;\n        print(\"Tuuuu sam\");\n      });\n    },\n\n    povecajItem(item) {\n      item.quantity += 1;\n      axios.put('http://127.0.0.1:5000/app/cart/povecaj', item).then(response => {\n        console.log('Ovo su podaci koji se dohvacaju' + response.data.data);\n      });\n    },\n\n    povecajItemUCart(item) {\n      this.povecajItem(item), this.dohvatiItem();\n    },\n\n    umanjiKolicinu(item) {\n      axios.post('http://127.0.0.1:5000/app/cart/umanji', item).then(response => {\n        console.log('Ovo su podaci koji se dohvacaju' + response.data.data);\n      });\n    },\n\n    urediCartSkripta() {\n      console.log(\"Usal sam v urediCartSkriptu\");\n      var artikli = this.items;\n\n      for (let i = 0; i < artikli.length; i++) {\n        console.log(\"Usal v for petlju\");\n        console.log(artikli[i]);\n      }\n    },\n\n    umanjiKolicinuCart(item) {\n      this.umanjiKolicinu(item);\n      this.dohvatiItem();\n    },\n\n    mounted() {\n      this.dohvatiItem();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDA;AACA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAGA,eAAc;EACZC,EAAE,EAAE,MADQ;;EAEZC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGC,CANS;;EAOZC,UAAU,EAAE;IACVL;EADU,CAPA;EAYZM,OAAO,EAAC;IACJC,WAAW,GAAE;MACdN,KAAK,CAACO,GAAN,CAAU,gCAAV,EACEC,IADF,CACQC,QAAD,IAAc;QACjBC,OAAO,CAACC,GAAR,CAAY,oCAAoCF,QAAQ,CAACP,IAAT,CAAcA,IAA9D;QACA,KAAKC,KAAL,GAAaM,QAAQ,CAACP,IAAtB;QACAU,KAAK,CAAC,WAAD,CAAL;MACF,CALF;IAOA,CATK;;IAWRC,WAAW,CAACC,IAAD,EAAO;MAChBA,IAAI,
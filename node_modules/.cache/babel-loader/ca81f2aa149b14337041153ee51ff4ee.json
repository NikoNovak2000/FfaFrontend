<tbody> \n                <tr v-for=\"(item, index) in items\" :key=\"index\"> \n                    <td>{{item.name}}</td>\n                    <td>{{item.price}} $</td>\n                    <td>{{item.img}}</td>\n                    <td>{{item.quantity}} </td>\n                    <td> \n                        <div class=\"btn-group\" role=\"group\">\n                            <button\n                            type=\"button\"\n                            class=\"btn btn-info btn-sm\"\n                            @click=\"povecajItemUCart(item)\"> Uvecaj </button>\n                        </div> \n                    </td>\n                    <td> \n                        <div class=\"btn-group\" role=\"group\">\n                            <button\n                            type=\"button\"\n                            class=\"btn btn-danger btn-sm\"\n                            @click=\"umanjiKolicinuCart(item)\"> Umanji </button>\n                        </div> \n                    </td>\n                </tr> \n            </tbody>\n\n        </table>\n\n  </div>\n</template>\n\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\nimport axios from 'axios'\n\n\nexport default{\n  el: '#app',\n  data() {\n    return {\n      items: []\n    }\n    },\n  components: {\n    HelloWorld\n  },\n \n \n  methods:{\n      dohvatiItem(){\n     axios.get('http://127.0.0.1:5000/app/cart')\n      .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n         this.items = response.data\n         print(\"Tuuuu sam\")\n      })\n      \n    },\n\n  povecajItem(item) {\n    item.quantity += 1;\n    axios.put('http://127.0.0.1:5000/app/cart/povecaj',item)\n      .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n      })\n  },\n\n   povecajItemUCart(item){\n    this.povecajItem(item),\n    this.dohvatiItem();\n  },\n\n  umanjiKolicinu(item){\n    axios.post('http://127.0.0.1:5000/app/cart/umanji',item)\n    .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n      })\n  },\n\n  urediCartSkripta(){\n\n    console.log(\"Usal sam v urediCartSkriptu\")\n    var artikli = this.items\n\n    for (let i = 0; i < artikli.length; i++) {\n      console.log(\"Usal v for petlju\")\n        console.log(artikli[i])\n      }\n  },\n\n  umanjiKolicinuCart(item){\n    this.umanjiKolicinu(item);\n    this.dohvatiItem();\n  },\n  mounted(){\n    this.dohvatiItem();\n  },\n}\n}\n\n\n\n\n</script>\n\n\n<style scoped>\n\n\n</style>"]},"metadata":{},"sourceType":"module"}
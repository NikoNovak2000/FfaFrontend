{"ast":null,"code":"// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue';\nimport axios from 'axios';\nexport default {\n  el: '#app',\n\n  data() {\n    return {\n      items: []\n    };\n  },\n\n  components: {\n    HelloWorld\n  },\n  methods: {\n    dohvatiItem() {\n      axios.get('http://127.0.0.1:5000/app/cart').then(response => {\n        console.log('Ovo su podaci koji se dohvacaju' + response.data.data);\n        this.items = response.data;\n      });\n    },\n\n    povecajItem(item) {\n      axios.post('http://127.0.0.1:5000/app/cart/povecaj', item).then(response => {\n        console.log('Ovo su podaci koji se dohvacaju' + response.data.data);\n        this.items = response.data;\n      });\n    },\n\n    povecajItemUCart(item) {\n      this.povecajItem(item), this.dohvatiItem();\n    }\n\n  },\n\n  mounted() {\n    this.dohvatiItem();\n  }\n\n};","map":{"version":3,"mappings":"AA2CA;AACA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAGA,eAAc;EACZC,EAAE,EAAE,MADQ;;EAEZC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGC,CANS;;EAOZC,UAAU,EAAE;IACVL;EADU,CAPA;EAWZM,OAAO,EAAC;IACJC,WAAW,GAAE;MACdN,KAAK,CAACO,GAAN,CAAU,gCAAV,EACEC,IADF,CACQC,QAAD,IAAc;QACjBC,OAAO,CAACC,GAAR,CAAY,oCAAoCF,QAAQ,CAACP,IAAT,CAAcA,IAA9D;QACA,KAAKC,KAAL,GAAaM,QAAQ,CAACP,IAAtB;MACF,CAJF;IAMA,CARK;;IAURU,WAAW,CAACC,IAAD,EAAO;MAChBb,KAAK,CAACc,IAAN,CAAW,wCAAX,EAAoDD,IAApD,EACGL,IADH,CACSC,QAAD,IAAc;QACjBC,OAAO,CAACC,GAAR,CAAY,oCAAoCF,QAAQ,CAACP,IAAT,CAAcA,IAA9D;QACA,KAAKC,KAAL,GAAaM,QAAQ,CAACP,IAAtB;MACF,CAJH;IAKD,CAhBO;;IAkBPa,gBAAgB,CAACF,IAAD,EAAM;MACrB,KAAKD,WAAL,CAAiBC,IAAjB,GACA,KAAKP,WAAL,EADA;IAED;;EArBO,CAXI;;EAmCdU,OAAO,GAAE;IACL,KAAKV,WAAL;EACF;;AArCY,CAAd","names":["HelloWorld","axios","el","data","items","components","methods","dohvatiItem","get","then","response","console","log","povecajItem","item","post","povecajItemUCart","mounted"],"sourceRoot":"","sources":["C:\\Users\\Niko Novak\\Desktop\\AppNarudzba\\ffa-frontend\\src\\views\\Cart.vue"],"sourcesContent":["<template>\n  <div class=\"Cart\">\n    <table class=\"table table-hover\"> \n            <thead> \n                <th scope=\"coll\">Naziv </th>\n                <th scope=\"coll\">Cijena </th>\n                <th scope=\"coll\">Slika </th>\n                <th scope=\"coll\">Kolicina </th>\n\n            </thead>\n\n            <tbody> \n                <tr v-for=\"(item, index) in items\" :key=\"index\"> \n                    <td>{{item.name}}</td>\n                    <td>{{item.price}} $</td>\n                    <td>{{item.img}} </td>\n                    <td>{{item.quantity}} </td>\n                    <td> \n                        <div class=\"btn-group\" role=\"group\">\n                            <button\n                            type=\"button\"\n                            class=\"btn btn-info btn-sm\"\n                            @click=\"dodajHranu(item)\"> Dodaj </button>\n                        </div> \n                    </td>\n                    <td> \n                        <div class=\"btn-group\" role=\"group\">\n                            <button\n                            type=\"button\"\n                            class=\"btn btn-danger btn-sm\"\n                            @click=\"izbrisiHranu(item)\"> Izbrisi </button>\n                        </div> \n                    </td>\n                </tr> \n            </tbody>\n\n        </table>\n\n  </div>\n</template>\n\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\nimport axios from 'axios'\n\n\nexport default{\n  el: '#app',\n  data() {\n    return {\n      items: []\n    }\n    },\n  components: {\n    HelloWorld\n  },\n \n  methods:{\n      dohvatiItem(){\n     axios.get('http://127.0.0.1:5000/app/cart')\n      .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n         this.items = response.data\n      })\n      \n    },\n\n  povecajItem(item) {\n    axios.post('http://127.0.0.1:5000/app/cart/povecaj',item)\n      .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n         this.items = response.data\n      })\n  },\n\n   povecajItemUCart(item){\n    this.povecajItem(item),\n    this.dohvatiItem();\n  },\n},\n\nmounted(){\n    this.dohvatiItem();\n  }\n\n}\n\n\n\n\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}
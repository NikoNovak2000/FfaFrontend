ipt>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\nimport axios from 'axios'\n\n\nexport default{\n  el: '#app',\n  data() {\n    return {\n      items: []\n    }\n    },\n  components: {\n    HelloWorld\n  },\n \n  methods:{\n     dohvatiItem(){\n     axios.get('http://127.0.0.1:5000/items')\n      .then((response) => {\n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n         this.items = response.data\n      })\n     }\n    ,\n\n  dodajItem(item) {\n    console.log(item)\n    axios.post(`http://127.0.0.1:5000/app/cart/dodajHranu/${item._id}`)\n      .then((response) => {\n        \n         console.log('Ovo su podaci koji se dohvacaju' + response.data.data)\n         this.items = response.data\n      })\n  },\n\n  dodajItemUCart(item){\n    this.dodajItem(item),\n    this.dohvatiItem();\n  },\n  \n},\n\nmounted(){\n    this.dohvatiItem();\n  }\n\n}\n\n\n\n\n</script>\n\n<style scoped lang=\"scss\">\n\n*{\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n}\n\nhtml{\n    height: 100%;\n}\n\nbody{\n    height: 100%;\n}\n\n.container{\n    display: grid;\n    grid-template-columns: 250px 1fr 350px;\n    height: 100%;\n    overflow: hidden;\n}\n\n.slikaLogo img{\n    width: 190px;\n    height: 180px;\n    padding-left: 20px;\n    padding-top: 20px;\n    margin-top: 35px;\n}\n\n.menuItem a{\n    display: block;\n    text-decoration: none;\n    color: #070303;\n    font-size: 20px;\n    padding: 0px 14px;\n    margin: 10px;\n}\n\n.menuItem a:hover{\n    background: #ffd3d3;\n}\n\n#menu{\n    background: #f34d2c;\n}\n\n.foodItems{\n   display: none;\n}\n\n#foodContainer{\n    background: #3ba552;\n    box-sizing: content-box;\n    overflow: scroll;\n    overflow-x: hidden;\n}\n\n#header{\n    background: #a3ffa8;\n    padding-top: 50px;\n    padding-bottom: 15px;\n    display: flex;\n    align-items: center;\n    position: sticky;\n    justify-content: space-between;\n    z-index: 1;\n}\n\n.yourAddress {\n    border:#070303 2px solid;\n    color: #070303;\n    padding: 4px 8px;\n    border-radius: 4px;\n    width: 250px;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    text-transform: capitalize;\n    cursor: pointer;\n    margin-left: 10px;\n    background: #ffffff;\n}\n \n.addBox i{\n    font-size: 16px;\n}\n\n.util i{\n    border:#006ba8 2px solid;\n    color: #006ba8;\n    font-size: 16px;\n    margin: 0 14px;\n    padding: 8px 16px;\n    cursor: pointer;\n    border-radius: 16px;\n    background: #ffffff;\n}\n\n.util i:active, .user i:active{\n    color:#ffffff;\n    background: #1797ff;\n}\n\n#hamburger,#cheeseburger,#baconburger,#chickenburger,#doubleburger{\n    margin-top: 40px;\n}\n\n\n\ntable{\n    width: 1000px !important;\n}\n#foodCategory, .menuItem{\n    margin: 14px;\n    font-size: 36px;\n    color: #ff520e;\n}\n\n#itemCard{\n    width: 240px;\n    height: 220px;\n    background: #b9bcdf;\n    padding: 8px;\n    margin: 8px;\n    transition: 05s all ease-in-out;\n    display: inline-block;\n    border-radius: 5px;\n    cursor: pointer;\n}\n\n#itemCard img{\n    width: 160px;\n    height: 100px;\n    display: block;\n    margin-bottom: 20px;\n    padding-bottom: 10px;\n    border-radius: 50%;\n    margin: auto;\n    transition: 0.5s all ease-in-out;\n}\n\n#itemCard img:hover{\n    transform: scale(1.2);\n}\n\n#itemName{\n    margin: 6px 0;\n    font-weight: 600;\n    color: #000000;\n    font-size: 20px;\n}\n\n#itemPrice{\n    margin: 0px;\n    font-weight: 550;\n    color: #000000;\n    font-size: 16px;\n}\n\n#cardTop {\n    display: flex;\n    margin: 8px 0;\n    justify-content: space-between;\n}\n\n#rating{\n    color: #fafa18;\n    padding: 5px;\n    background: #070303;\n    font-size: 12px;\n    border-radius: 12px;\n}\n\n.fa-heart-o{\n    padding: 6px;\n    background: #ffffff;\n    cursor: pointer;\n    border-radius: 50%;\n}\n\n.toggle-heart{\n    background: #ff0000;\n    color: #ffffff;\n}\n\n::-webkit-scrollbar{\n    display: none;\n}\n\n#cart{\n    background: #f34d2c;\n    padding-right: 30px;\n    overflow-y: scroll;\n}\n\n.fa-user-circle{\n    border:#006ba8 2px solid;\n    color: #006ba8;\n    font-size: 16px;\n    margin: 0 14px;\n    padding: 8px 18px;\n    cursor: pointer;\n    border-radius: 16px;\n    background: #ffffff;\n    margin-top: 50px;\n    margin-bottom: 10px;\n}\n\n.taste-header{\n    padding-top: 40px;\n    padding-bottom: 10px;\n    position: sticky;\n    top: 0;\n    background: #ffffff;\n}\n\n.itemMenu{\n    margin: 0;\n    font-size: 30px;\n    margin-top: 40px;\n    color: #d4d4d4;\n}\n\n.border{\n    width: 160px;\n    height: 4px;\n    margin: 3px 0;\n    margin-bottom: 30px;\n    border-radius: 2px;\n    background-color: #181414;\n}\n\n.listCard{\n    display: flex;\n    align-items: center;\n    margin-top: 15px;\n    border-radius: 25px;\n    cursor: pointer;\n    transition: 0.1s all ease-in-out;\n}\n\n.listCard img{\n    width: 80px;\n    height: 50px;\n    border-radius: 50%;\n}\n\n.listCard:hover{\n    background: white;\n}\n\n.listCard:hover img{\n    transition: 0.3s ease-in-out;\n    transform: scale(1.2);\n}\n\n.listName{\n    font-size: 18px;\n    margin: 0 10px;\n    text-decoration: none;\n    text-transform: capitalize;\n    color: #070303;\n}\n\n#cartPage {\n     background: rgb(255, 255, 255);\n     margin: 40px 10px;\n     margin-right: 50px;\n     padding: 20px 30px;\n     padding-bottom: 30px;\n     position: relative;\n}\n\n#cartTitle{\n    font-size: 35px;\n    color: red;\n}\n\n#cartPage table{\n    width: 100%;\n    margin-top: 20px;\n    text-align: center;\n}\n\n#cartPage table head td{\n    font-size: 20px;\n    padding-bottom: 10px;\n}\n\n#mTotalAmount{\n    display: none;\n}\n\n#cartPage table tbody td{\n    padding: 10px 0;\n    font-size: 20px;\n}\n\n#cartPage table tbody button{\n    outline: none;\n    border: none;\n    font-size: 16px;\n    background: none;\n    background: lightcoral;\n    color: white;\n    border-radius: 3px;\n    cursor: pointer;\n    margin: 0 5px;\n    padding: 3px 8px; \n}\n\n#cartPage table tbody img{\n    width: 80px;\n    height: 80px;\n    border-radius: 50px;\n}\n\n.cartToggle{\n    display: none;\n}\n\n#checkout{\n    text-align: center;\n    margin: 40px auto;\n    border-radius: 5px;\n    background: white;\n    padding: 10px 20px;\n}\n\n#checkout p{\n    margin: 15px 0;\n    float: left;\n}\n\n#totalPrice,#totalItem{\n    font-size: 20px;\n    color: red;\n}\n\n.checkoutBtn{\n    outline: none;\n    border: none;\n    background: #26a2eb;\n    padding: 10px 25px;\n    border-radius: 20px;\n    font-size: 16px;\n    bottom: 30px;\n    margin: 15px 0;\n    cursor: pointer;\n}\n\n.checkoutBtn:active{\n    color:#52d9e2;\n    background: none;\n    border: 1px solid #52d9e2;\n}\n\n#delievery{\n    font-size: 16px;\n    color: #00ac00;\n}\n</style>"]},"metadata":{},"sourceType":"module"}